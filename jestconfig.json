{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "transformIgnorePatterns": ["node_modules/(?!axios)"],
  "testEnvironment": "jsdom",
  "setupFilesAfterEnv": ["<rootDir>/jest-startup.js"]
}
