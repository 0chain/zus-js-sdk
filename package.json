{
  "name": "zus-sdk",
  "version": "0.0.6",
  "description": "[![GitHub license](https://img.shields.io/badge/license-MIT-blue.svg)](./LICENSE) [![PRs Welcome](https://img.shields.io/badge/PRs-welcome-brightgreen.svg)](./CONTRIBUTING.md)",
  "main": "./lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "scripts": {
    "start": "node src/example.js",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*",
    "LICENSE",
    "AUTHORS",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0chain/zus-js-sdk.git"
  },
  "keywords": [
    "zus-js-sdk",
    "js-client-sdk"
  ],
  "author": "0Chain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0chain/zus-js-sdk/issues"
  },
  "homepage": "https://github.com/0chain/zus-js-sdk#readme",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.4.0",
    "jest-mock-axios": "^4.7.0-beta2",
    "prettier": "^2.5.0",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "bluebird": "^3.7.2",
    "js-sha3": "^0.8.0",
    "json-bigint": "^1.0.0",
    "moment": "^2.29.4"
  }
}
